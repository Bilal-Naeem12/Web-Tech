            <input type="text" class="form-control my-2 " id="createUrl" required placeholder="New Image Url">
        </div>
        <div class="form-group">
            <label for="createContent">Description:</label>
            <textarea class="form-control my-2 " id="createDesc" rows="3" required>Enter Description</textarea>
        </div>
