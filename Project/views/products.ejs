          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <form id="createForm">
            <div class="form-group">
              <label for="createContent">Description:</label>
              <textarea id="update-text-area" class="form-control my-2 " id="createContent" rows="3" required>Enter Description</textarea>
          </div>
        </form>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          <button id="update-btn" type="button" class="btn btn-dark ">Save changes</button>
        </div>
      </div>
    </div>
  </div>
 
  
